<div class="headerTimeViewlet"
     i18n:domain="vnccollab.theme">
  <span class="date"
     i18n:domain="plonelocales">
    <span class="dayname"
          tal:content="view/dayname"
          i18n:translate="">Friday</span>
    <span class="day"
          tal:content="view/day"
          i18n:translate="">01</span>
    <span class="month"
          tal:content="view/month"
          i18n:translate="">November</span>
  </span>
  <a class="closeLink"
     title="Click to contract"
     i18n:attributes="title"
     href="#">
    <img src="arrow_up_white.png"
         alt="Close"
         i18n:attributes="alt"
         tal:attributes="src string:${view/site_url}/${attrs/src}" />
  </a>
</div>
