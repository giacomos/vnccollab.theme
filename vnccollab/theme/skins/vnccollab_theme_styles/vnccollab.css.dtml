  }
a#toggle-online-users:after {
  content: "";
  }
#toggle-online-users span {
  display: inline-block;
  vertical-align: middle;
  }
#online-count {
  background-color: #75a724;
  border: 1px solid #558114;
  border-radius: 16px;
  color: #fff;
  min-width: 10px;
  padding: 0 3px;
  text-align: center;
  }
#im-messages span {
  display: inline-block;
  vertical-align: middle;
  }
#unread-messages {
  background-color: #75a724;
  border: 1px solid #558114;
  border-radius: 16px;
  color: #fff;
  min-width: 10px;
  padding: 0 3px;
  text-align: center;
  }
#online-users {
  background-color:#fff;
  border:1px solid #c1ccd5;
  right: -8px;
  }

div.postItemWrapper {
  background-color:rgba(1, 42, 67, 0.3);
  border-bottom:1px solid #fff;
  -moz-border-radius:1em;
  border-radius:1em;
  margin:20px 0 1em 95px;
  max-width:574px;
  padding:1px;
  }
#content h1.documentFirstHeading {
  text-transform: capitalize;
  font-weight: 400;
}
#content .postItemWrapper fieldset {
  border:0;
  -moz-border-radius:1em 1em 1em 0;
  border-radius:1em 1em 1em 0;
  padding:10px;
  }
#content .postItemWrapper textarea {
  font:18px "myriad pro", verdana, sans-serif;
  padding:5px;
  width:99%;
  }
.share-geolocation {
  color:#fff;
  margin-top:3px;
