@media screen {

/*---------- General Style ----------*/
body {
  background: #e6e9ee;
  color:#000;
  font:100% "Myriad Pro", Arial, FreeSans, sans-serif;
  min-width: 1060px;
  }
#ploneimage,
#plonelink {
  min-width: 0;
  }
#portal-top {
  width: 100%;
  }
#portal-header {
  margin: 0;
  }
#portal-columns {
  background-color: #f2f2f2;
  border-top: 1px solid rgba(0, 42, 67, 0.3);
  border-bottom: 1px solid #c1ccd5;
  padding: 22px 0 50px;
  position: relative;
  width: 100%;
  }
#portal-columns:after,
#portal-footer:after {
  clear: both;
  content: "";
  display: block;
  visibility: hidden;
  }
#portal-columns.width-3\3a 4.position-0:before,
#portal-columns.width-1\3a 2:before {
  background: url(&dtml-portal_url;/columns-separator.png) no-repeat;
  background-size: 13px 100%;
  bottom: 60px;
  content: "";
  display: block;
  position: absolute;
  right: 330px;
  top: 30px;
  width: 30px;
  }
#visual-column-wrapper {
  float: left;
  margin-right: -360px;
  width: 100%;
  }
#portal-column-content {
  float: right;
  margin-left: -360px;
  width: 100%;
  }
#portal-content {
  margin: 0 390px 0 30px;
  }
.width-full #portal-content {
  margin: 0 30px;
  }
.width-1\3a 2 #portal-content {
  margin: 0 390px;
  }
.width-3\3a 4.position-1\3a 4 #portal-content {
  margin: 0 30px 0 390px;
  }
#portal-column-one {
  float: left;
  margin: 0 30px;
  width: 300px;
  }
#portal-column-two {
  float: right;
  margin: 10px 30px 0 0;
  width: 300px;
  }
#portal-footer {
  background: none;
  margin: 15px auto;
  padding: 0;
  width: 960px;
  }

/*----- Personal Tools -----*/
#portal-personaltools-loggedin {
  font-size:13px;
  line-height:18px;
  position: absolute;
  right: 30px;
  top: 52px;
  z-index:1;
  }
.actionMenuUserImgWrapper {
  border: 1px solid #738ea2;
  border-right: 0;
  float:left;
  border-top-left-radius: 4px;
  border-bottom-left-radius: 4px;
  }
#user-image img {
  height: 32px;
  vertical-align: top;
  width: auto;
  }
.userPreferences {
  background-color: #dee3e8;
  background-image: -moz-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -ms-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -o-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -webkit-linear-gradient(#dee3e8,#cfd6dd);
  background-image: linear-gradient(#dee3e8,#cfd6dd);
  border: 1px solid #738ea2;
  box-shadow: 0 1px #e6e9ee inset;
  float: left;
  margin-right: 10px;
  position: relative;
  width: 149px;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
  }
.userPreferencesHeader {
  color: #738ea2;
  cursor:pointer;
  font-size: 14px;
  font-weight:normal;
  line-height: 18px;
  padding: 7px 24px 7px 9px;
  position:relative;
  }
.userPreferencesHeader:after {
  background:url(&dtml-portal_url;/sprite.png) no-repeat -206px -20px;
  content: "";
  display: block;
  height: 8px;
  position: absolute;
  right: 7px;
  top: 12px;
  width: 13px;
  }
.userPreferencesHeader a {
  display: block;
  overflow: hidden;
  white-space: nowrap;
  }
.userPreferencesBody {
  border-top: 0;
  display:none;
  margin:-1px 0 0;
  padding:4px 0;
  }
.userPreferences:hover .userPreferencesBody {
  display:block;
  }
.userPreferencesBody a {
  display: block;
  padding: 5px 9px;
  }
.userPreferencesBody a:before {
  background:url(&dtml-portal_url;/sprite.png) no-repeat -190px 0;
  content: "";
  display: inline-block;
  height: 16px;
  margin-right: 5px;
  vertical-align: text-top;
  width: 16px;
  }
.userPreferencesBody a:hover {
  text-decoration:underline;
  }
#personaltools-mystuff a:before {
  background-position: -190px -20px;
  }
#personaltools-dashboard a:before {
  background-position: -206px 0;
  }

/*----- Language Selector -----*/
#portal-languageselector {
  background-color: #dee3e8;
  background-image: -moz-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -ms-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -o-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -webkit-linear-gradient(#dee3e8,#cfd6dd);
  background-image: linear-gradient(#dee3e8,#cfd6dd);
  border: 1px solid #738ea2;
  border-radius: 4px;
  box-shadow: 0 1px #e6e9ee inset;
  float: left;
  font-size: 14px;
  font-weight:normal;
  line-height: 18px;
  margin-right: 10px;
  padding: 28px 24px 4px 0;
  width: 92px;
  }
#portal-languageselector:after {
  background:url(&dtml-portal_url;/sprite.png) no-repeat -206px -20px;
  content: "";
  display: block;
  height: 8px;
  position: absolute;
  right: 7px;
  top: 12px;
  width: 13px;
  }
#portal-languageselector li {
  display:block;
  }
#portal-languageselector a {
  display: none;
  padding: 3px 9px;
  }
#portal-languageselector:hover a {
  display: block;
  overflow: hidden;
  white-space: nowrap;
  }
#portal-languageselector .currentLanguage a {
  display: block;
  left: 0;
  overflow: hidden;
  position: absolute;
  top: 4px;
  white-space: nowrap;
  }

/*----- Logout Link -----*/
.logoutLink {
  background: url(&dtml-portal_url;/sprite.png) no-repeat -222px 0 #12425f;
  display: inline-block;
  height: 34px;
  overflow: hidden;
  text-indent: -9999px;
  width: 34px;
  border-radius: 4px;
  }

/*----- Login -----*/
.loginFormWrapper {
  position: absolute;
  right: 0;
  top: 52px;
  }
.loginFormControls label {
  font-weight:normal;
  margin:0 0 3px;
  vertical-align:middle;
  }
.loginFormControls input::-webkit-input-placeholder {
  color: #738ea2;
  }
input.loginFormName,
input.loginFormEmail,
input.loginFormPassword {
  background-color: #dee3e8;
  background-image: -moz-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -ms-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -o-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -webkit-linear-gradient(#dee3e8,#cfd6dd);
  background-image: linear-gradient(#dee3e8,#cfd6dd);
  border: 1px solid #738ea2;
  box-shadow: 0 1px #e6e9ee inset;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  color: #738ea2;
  display: inline-block;
  font-size: 14px;
  height: 32px;
  line-height: 17px;
  margin-right: 7px;
  padding: 6px 9px;
  vertical-align: top;
  width: 140px;
  }
button::-moz-focus-inner,
input[type="reset"]::-moz-focus-inner,
input[type="button"]::-moz-focus-inner,
input[type="submit"]::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner {
  padding: 0;
  border: 0;
  }
.loginFormSubmit {
  background-color: #12425f;
  border: 1px solid #012a43;
  color:#fff;
  cursor: pointer;
  font-size: 14px;
  height: 32px;
  padding: 0 9px;
  text-align:center;
  vertical-align: top;
  }

/*----- Logo -----*/
#portal-logo {
  margin:0 0 0 30px;
  }

/*----- Searchbox -----*/
#portal-searchbox {
  background-color:#f5f5f5;
  border:1px solid #738ea2;
  border-radius: 3px;
  box-shadow: 1px 1px 2px #d9d9d9 inset;
  font-size:1em;
  margin: 0 -134px 0 0;
  padding: 6px 4px;
  position: absolute;
  right: 50%;
  top: 51px;
  width: 268px;
  }
#portal-searchbox .searchField {
  background-color:#f5f5f5;
  border:0;
  color:#738ea2;
  font-size:17px;
  outline:none;
  padding: 0 7px;
  vertical-align:top;
  width:230px;
  }
input.searchField {
  -moz-appearance: none;
  }
#portal-searchbox .searchButton {
  background:url(&dtml-portal_url;/sprite.png) no-repeat -154px 0;
  border:0;
  height:0;
  overflow:hidden;
  padding-top:20px;
  vertical-align:top;
  width:19px;
  }
.searchSection {
  display:none;
  }
#LSResult {
  background:none;
  margin:0;
  right: -125px;
  top:30px;
  z-index: 1;
  }
#LSResult fieldset {
  border:0;
  margin:0;
  padding:0 0 1em;
  }
#livesearchLegend {
  background-color: #f5f5f5;
  border: 0;
  box-shadow: 3px 0 2px -2px #D9D9D9 inset;
  color: #f5f5f5;
  height: 6px;
  left: 135px;
  margin:0;
  overflow: hidden;
  padding: 0;
  position:absolute;
  top: -5px;
  width: 276px;
  }
.LSIEFix {
  background: #f5f5f5;
  border: 1px solid #738ea2;
  border-radius: 3px;
  box-shadow: 0 3px 8px rgba(1, 42, 67, 0.25);
  padding: 11px 0 0;
  }
.LSRow,
#LSNothingFound {
  padding:0 17px;
  width: 500px;
  }
.LSRow a:link {
  color: #012a43;
  vertical-align: middle;
  }
.icons-on .livesearchContainer .contenttype-news-item {
  background-position: 0 -1288px;
  }
.icons-on .livesearchContainer .contenttype-document {
  background-position: 0 7px;
  }
.icons-on .livesearchContainer .contenttype-folder {
  background-position: 0 -640px;
  }
.icons-on .livesearchContainer .contenttype-topic {
  background-position: 0 -1720px;
  }
.icons-on .livesearchContainer .contenttype-link {
  background-position: 0 -1072px;
  }
.icons-on .contenttype-event {
  background-position: 0 -208px;
  }
.LSRow a[href="search_form"] {
  background-color: #e7e9ec;
  border-top: 1px solid #d9dfe3;
  display: block;
  font-size: 12px;
  font-weight: bold !important;
  line-height: 16px;
  margin: 10px -17px 0;
  padding: 12px 17px 6px;
  }
.LSRow a[href="search_form"]:before {
  background: url(&dtml-portal_url;/sprite.png) no-repeat -173px 0;
  content: "";
  display: inline-block;
  height: 16px;
  margin-right: 10px;
  vertical-align: middle;
  width: 15px;
  }
.LSRow a[href^="search?SearchableText="] {
  background-color: #e7e9ec;
  display: block;
  font-size: 12px;
  font-weight: bold !important;
  line-height: 16px;
  margin: 0 -17px;
  padding: 0 17px 11px;
  }
.LSRow a[href^="search?SearchableText="]:before {
  background: url(&dtml-portal_url;/sprite.png) no-repeat -173px -20px;
  content: "";
  display: inline-block;
  height: 16px;
  margin-right: 10px;
  vertical-align: middle;
  width: 15px;
  }
.LSRow:last-child a {
  border-radius: 0 0 3px 3px;
  padding-bottom: 11px;
  }
#LSResult .LSDescr {
  color: #012a43;
  font-size: 12px;
  line-height: 16px;
  }

/*----- Global Navigation -----*/
#portal-globalnav {
  background:#012a43;
  clear:none;
  font-size: 14px;
  line-height:23px;
  margin:0;
  box-sizing: border-box;
  padding: 0 30px 0 30px;
  text-transform: uppercase;
  }
#portal-globalnav li a {
  background-color:transparent;
  border:0;
  color:#fff;
  min-width:0;
  padding:11px 15px;
  }
#portal-globalnav li a:hover,
#portal-globalnav li.selected a,
#portal-globalnav li.selected:hover a {
  background-color:#6ea21b;
  color:#fff;
  }

/*----- VNC Collab Header -----*/
#vnccollab-header {
  background:url(&dtml-portal_url;/shadow.png) no-repeat 50% 100%;
  background-size: 100% 25px;
  margin: 0 30px;
  padding:0 0 25px;
  position:relative;
  }
.nccollab-hp-header .actionLink {
  display:none;
  }
.hp-header-content {
  background:url(&dtml-portal_url;/vnccollab-header.jpg) no-repeat 0 0 #022c45;
  color:#fff;
  font-size:13px;
  height:323px;
  line-height:18px;
  padding:0 15px 0 325px;
  position:relative;
  }
.hp-header-welcome {
  padding:22px 0 0;
  }
.hp-header-welcome h2 {
  color:#fff;
  font-size:28px;
  font-weight:normal;
  line-height:35px;
  margin:0 0 14px;
  }
.hp-header-welcome p {
  margin:0 0 14px;
  }
.hp-header-links {
  bottom: 0;
  height: 89px;
  left: 317px;
  position: absolute;
  right: 0;
  }
.hp-header-first-link {
  background-color:#6b9512;
  bottom:0;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  height:89px;
  left:0;
  padding:0 19px;
  position:absolute;
  width:50%;
  }
.hp-header-second-link {
  background-color:#164e6f;
  bottom:0;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  height:89px;
  padding:0 19px;
  position:absolute;
  right: 0;
  width:50%;
  }
.hp-header-first-link h3,
.hp-header-second-link h3 {
  font-size:28px;
  font-weight:normal;
  line-height:35px;
  margin:7px 0 14px;
  }
.hp-header-first-link h3 a,
.hp-header-second-link h3 a {
  color:#fff;
  }
#vnccollab-header.opened .headerTimeViewlet,
#vnccollab-header.opened .headerLinksViewlet,
#vnccollab-header.opened .carousel,
#vnccollab-header.opened .worldClockViewlet,
#vnccollab-header.closed .headerTimeViewletShort {
  display: block;
  }
#vnccollab-header.closed .headerTimeViewlet,
#vnccollab-header.closed .headerLinksViewlet,
#vnccollab-header.closed .carousel,
#vnccollab-header.closed .closeLink,
#vnccollab-header.closed .worldClockViewlet,
#vnccollab-header.opened .headerTimeViewletShort {
  display: none;
  }
.headerTimeViewlet {
  background:url(&dtml-portal_url;/clock.png) no-repeat 100% 0;
  color:#fff;
  float:right;
  font-size:1.06em;
  line-height:1.41em;
  margin:40px 0 0;
  padding:20px 80px 20px 20px;
  width:200px;
  }
.headerTimeViewlet span {
  display:inline-block;
  vertical-align:middle;
  }
.headerTimeViewlet .day {
  background-color:#fff;
  border-radius:15px;
  -moz-border-radius:15px;
  color:#082736;
  font-weight:bold;
  padding:4px 7px 3px 6px;
  }
.headerTimeViewletShort {
  background-color:#12425f;
  color:#fff;
  font-size:1.06em;
  line-height:1.41em;
  padding:2px 40px;
  text-align:right;
  }
.actionLink {
  background:url(&dtml-portal_url;/sprite.png) no-repeat;
  height:18px;
  overflow:hidden;
  position:absolute;
  right:11px;
  text-indent:-9999px;
  top:5px;
  width:18px;
  }
.openLink {
  background-position:0 -18px;
  text-indent:9999px;
  }
div.carousel {
  background-color:#12425f;
  margin:0;
  }
.carousel-banners {
  min-height: 117px;
  }
.carousel-image {
  float:left;
  }
div.carousel-text {
  background-color:transparent;
  color:#fff;
  display:table;
  font-size:0.81em;
  line-height:1.38em;
  padding:18px 40px 15px 22px;
  position:static;
  width:auto;
  }
div.carousel-text a:link,
div.carousel-text a:visited {
  color:#fff;
  text-decoration: underline;
  }
.carousel-title {
  color:#fff;
  display:block;
  font-size:1.46em;
  line-height:1.32em;
  margin:0 0 13px;
  }
.carousel-title:link,
.carousel-title:visited {
  color:#fff;
  }
div.carousel-pager {
  background-color:#a4b4c1;
  margin:0;
  padding:5px 15px;
  text-align:right;
  }
a.carousel-pager-button-prev,
a.carousel-pager-button-next {
  background-color:transparent;
  display:inline-block;
  padding:0;
  vertical-align:top;
  }
a.carousel-pager-button-prev {
  margin-right:5px;
  }
a.carousel-pager-button-prev span,
a.carousel-pager-button-next span {
  background:url(&dtml-portal_url;/sprite.png) no-repeat -18px 0;
  display:block;
  height:20px;
  overflow:hidden;
  text-indent:9999px;
  width:20px;
  }
a.carousel-pager-button-next span {
  background-position:-18px -20px;
  }
  
.headerLinksViewlet {
  bottom:10px;
  font-size:0.69em;
  line-height:1.45em;
  position:absolute;
  right:360px;
  }
.headerLinksViewlet li {
  margin-right:2px;
  }
.headerLinksViewlet a {
  background-color:#2a6181;
  border-radius:10px;
  -moz-border-radius:10px;
  color:#fff;
  display:inline-block;
  padding:2px 9px;
  }
.headerLinksViewlet a:visited {
  color:#fff;
  }

/*----- Bread Crumbs -----*/
#portal-breadcrumbs {
  background: rgba(0, 42, 67, 0.1);
  background-size: 100% 25px;
  font-size:12px;
  line-height: 20px;
  margin:0;
  padding: 10px 0px 10px 45px;
  text-transform: uppercase;
  }
#breadcrumbs-you-are-here {
  background-color:#012a43;
  -moz-border-radius:9px 0 0 9px;
  border-radius:9px 0 0 9px;
  color:#fff;
  display:inline-block;
  margin-right:10px;
  padding:0 0 0 14px;
  position:relative;
  }
#breadcrumbs-you-are-here:after {
  background:url(&dtml-portal_url;/bc.gif) no-repeat 100% 0;
  content:"";
