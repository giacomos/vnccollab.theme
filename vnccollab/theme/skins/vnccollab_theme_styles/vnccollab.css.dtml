  margin: 0;
  list-style-type: none;
}
dl.portlet dd.portletFooter {
  background:url(&dtml-portal_url;/tab.png) repeat-x 0 -37px;
  font-size:13px;
  line-height:18px;
  padding:6px 0 13px;
  position:absolute;
  right:31px;
  top:100%;
  }
.portletFooter .portletBottomLeft {
  background:url(&dtml-portal_url;/tab.png) repeat-x 0 0;
  height:37px;
  left:-16px;
  position:absolute;
  top:0;
  width:16px;
  }
.portletFooter .portletBottomRight {
  background:url(&dtml-portal_url;/tab.png) repeat-x 0 -74px;
  height:37px;
  position:absolute;
  right:-16px;
  top:0;
  width:16px;
  }
dl.portlet dd.portletFooter a {
  color:#fff !important;
  }
.portletItemDetails {
  color:rgba(1, 42, 67, 0.5);
  font-size:100%;
  margin-left:0;
  padding-top:0;
  text-shadow:0 1px 1px #fff;
  }
.portletStaticText .portletItem a:link,
.portletStaticText .portletItem a:hover,
.portletStaticText .portletItem a:visited {
  color:#748fa2 !important;
  }

/*----- Portlet Toggle Styles -----*/
.portlet .portletToggleLink {
