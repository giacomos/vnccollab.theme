  margin-left: 0;
  }
.MetaContent form label,
.MetaContent .label {
  color: rgba(255, 255, 255, 0.8);
  display: inline-block;
  font-weight: normal;
  font-size: 14px;
  letter-spacing: 1px;
  }
.MetaContent a.Item {
  background: rgba(255, 255, 255, 0.0);
  border-radius: 5px;
  color: rgba(255, 255, 255, 0.5);
  display: block;
  padding: 10px;
  -webkit-transition:all 0.2s ease-out;  
  -moz-transition:all 0.2s ease-out;  
  -o-transition:all 0.2s ease-out;  
  transition:all 0.2s ease-out;
  }
.MetaContent a.Item:hover {
  background: rgba(255, 255, 255, 0.1);
  color: rgba(255, 255, 255, 1.0);
  }
.MetaContent a.Item p.Label {
  color: #fff;
  font-size: 14px;
  font-weight: 600;
  margin-bottom: 2px;
  }
.MetaContent a.Item p.Desc {
  font-size:11px;
  margin:0;
  }
.MetaContent a.Item:Hover p.Desc {
  font-size:13px;
  }
.MetaContent a.Item img {
  float:left;
  margin-right:13px;
  opacity: 0.4;
  }
.MetaContent a.Item:hover img {
  height:auto;
  margin-right:10px;
  opacity: 1;
  width: 35px;
  }
.MetaContent form input {
  background: rgba(255, 255, 255, 0.7);
  border: 1px solid rgba(255, 255, 255, 1.0);
  border-radius: 5px;
  color: rgba(1,42,67,1.0);
  font-size:14px;
  margin-top: -6px;
  padding: 6px;
  }
#destination {
  margin-bottom: 4px;
  width: 100%;
  }
.MetaContent .DropZone {
  border:3px dashed rgba(255,255,255,0.3);
  border-radius:10px;
  padding: 50px 10px;
  text-align: center;
  }
.MetaContent .DropZone input {
  max-width: 250px;
  }
.MetaBoxOverlay {
  background-color: #aaa;
  bottom: -1px;
  display: none;
  left: -1px;
  opacity: 0.5;
  position: absolute;
  right: -1px;
  top: -1px;
  z-index: 10;
  }
.ajaxLoadImage {
  left: 50%;
  height: 32px;
  margin: -16px 0 0 -16px;
  position: absolute;
  top: 50%;
  width: 32px;
  z-index: 103;
  }
.spinnerAddContentArea {
  height: 100px;
  margin: -50px 0 0 -50px;
  width: 100px;
  }
.PullTab {
  margin: 0 auto 10px;
  height: 37px;
  opacity: 0.6;
  -webkit-transition:opacity 0.2s ease-out;  
  -moz-transition:opacity 0.2s ease-out;  
  -o-transition:opacity 0.2s ease-out;  
  transition:opacity 0.2s ease-out;
  width: 194px;
  }
.PullTab:hover,
.PullTab.open {
  opacity: 1.0;
  }
.PullTab a:focus img {
  outline: none;
  }
  
}

