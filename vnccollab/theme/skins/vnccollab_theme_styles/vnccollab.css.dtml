  }
.vncStreamItem:hover {
  background-color: #f2f2f2;
  }
.vncStreamItemImg {
  float: left;
  height: 32px;
  width: 32px;
  }
.vncStreamItemBody {
  margin-left: 42px;
  }
.vncStreamItemByline {
  display: block;
  font-size: 0.875em;
  }
.vncStreamItemTitle {
  display: block;
  color: #75ad0a !important;
