@media screen {

/*---------- General Style ----------*/
body {
  background:url(&dtml-portal_url;/bg.png) repeat-x #c7d0da;
  color:#000;
  font:100% "Myriad Pro", Arial, FreeSans, sans-serif;
  }
#visual-portal-wrapper {
  position:relative;
  }
#visual-portal-wrapper:after {
  content:"";
  clear:both;
  display:block;
  }
#portal-header {
  margin: auto;
  width: 960px;
  }

/*----- Layout -----*/
div.width-full {
  width:960px;
  }
.cell div.width-full {
  width:100%;
  }
.cell div.position-0 {
  margin-left:-100%;
  }
div.position-0 {
  margin-left:-960px;
  }
div.position-1\3a 4 {
  margin-left:-620px;
  }
div.position-3\3a 4 {
  margin-left:-310px;
  }
div.width-1\3a 4 {
  width:310px;
  }
div.width-1\3a 2 {
  width:260px;
  }
div.width-3\3a 4 {
  width:635px;
  }
#portal-columns {
  border-top:1px solid #e6eaee;
  left: 50%;
  margin-left: -480px;
  width: 960px;
  }
#portal-columns + .row {
  clear: left;
  left: 50%;
  margin-left: -480px;
  width: 960px;
  }

/*----- Personal Tools -----*/
#portal-personaltools-anonymous {
  font-size:11px;
  line-height:16px;
  position:absolute;
  right:15px;
  top:26px;
  }
#portal-personaltools-anonymous a {
  color:#082736;
  display:inline !important;
  margin-left:5px;
  text-decoration:underline;
  }
#portal-personaltools-loggedin {
  background-color:#cfd6dd;
  float:right;
  font-size:13px;
  height:80px;
  line-height:18px;
  margin:10px 0 15px;
  padding:0 15px 0 0;
  position:relative;
  width:325px;
  z-index:1;
  }
#portal-personaltools-loggedin a {
  color:#082736;
  }
.actionMenuUserImgWrapper {
  float:left;
  margin-right:15px;
  }
.userHeader {
  margin:12px 0 16px;
  }
#user-name {
  font-weight:bold;
  }
.userPreferences {
  display:table;
  position:relative;
  width:149px;
  }
.userPreferencesHeader {
  background:url(&dtml-portal_url;/dd.gif) no-repeat 100% 0 #cfd6dd;
  border:1px solid #b3c0ca;
  cursor:pointer;
  font-weight:normal;
  padding:3px 30px 2px 10px;
  position:relative;
  z-index:1;
  }
.userPreferencesBody {
  background-color:#e6e9ee;
  border:1px solid #b3c0ca;
  -moz-box-shadow:0 2px 8px  rgba(0,0,0,0.75);
  -webkit-box-shadow:0 2px 8px  rgba(0,0,0,0.75);
  box-shadow:0 2px 8px  rgba(0,0,0,0.75);
  display:none;
  margin:-1px 0 0;
  padding:4px 0;
  position:absolute;
  width:147px;
  }
.userPreferences:hover .userPreferencesBody {
  display:block;
  }
.userPreferencesBody a {
  background:url(&dtml-portal_url;/useractions-icons.png) no-repeat 10px 8px;
  display:block;
  padding:7px 0 7px 35px;
  }
.userPreferencesBody a:hover {
  text-decoration:underline;
  }
#personaltools-mystuff a {
  background-position:10px -93px
  }
#personaltools-dashboard a {
  background-position:10px -192px
  }
.logoutLink {
  background-color:#12425f;
  border:1px solid #012a43;
  color:#fff !important;
  display:inline-block;
  font-size:11px;
  line-height:17px;
  padding:2px 0;
  position:absolute;
  right:15px;
  text-align:center;
  top:12px;
  width:58px;
  }

/*----- Language Selector -----*/
#portal-languageselector {
  background:none;
  bottom:10px;
  padding:0;
  position:absolute;
  right:15px;
  top:auto;
  }
#portal-languageselector li {
  background:url(&dtml-portal_url;/flags.png) no-repeat 0 0;
  display:inline-block;
  margin-left:10px;
  vertical-align:bottom;
  }
#portal-languageselector .language-en {
  background-position:-23px 0;
  }
#portal-languageselector .currentLanguage.language-en {
  background-position:-23px -23px;
  }
#portal-languageselector .currentLanguage.language-de {
  background-position:0 -23px;
  }
#portal-languageselector a {
  display:block;
  height:23px;
  width:23px;
  overflow:hidden;
  }
#portal-languageselector img {
  margin-left:-23px;
  }

/*----- Login -----*/
.loginFormWrapper {
  background-color:#cfd6dd;
  float:right;
  font-size:11px;
  height:60px;
  line-height:18px;
  margin:10px 0 15px;
  padding:12px 15px 8px;
  width:310px;
  }
.loginFormHeader {
  margin:0 0 14px;
  }
.loginFormLink {
  float:right;
  text-decoration:underline;
  }
.loginFormLink:link,
.loginFormLink:visited {
  color:#082736;
  }
.loginFormTitle {
  font-size:13px;
  font-weight:bold;
  line-height:19px;
  }
.loginFormControls label {
  font-weight:normal;
  margin:0 0 3px;
  vertical-align:middle;
  }
.loginFormName,
.loginFormEmail,
.loginFormPassword {
  background-color:#edeff2;
  border:0;
  -moz-box-sizing:border-box;
  -webkit-box-sizing:border-box;
  box-sizing:border-box;
  font-family:Arial,FreeSans,sans-serif;
  font-size:11px;
  height:27px;
  padding:1px 3px;
  vertical-align:middle;
  width:90px;
  }
.loginFormName,
.loginFormEmail {
  margin-right:5px;
  }
.loginFormSubmit {
  background-color:#12425f;
  border:0;
  -moz-box-sizing:border-box;
  -webkit-box-sizing:border-box;
  box-sizing:border-box;
  color:#fff;
  height:27px;
  text-align:center;
  vertical-align:middle;
  width:27px;
  }

/*----- Logo -----*/
#portal-logo {
  margin:0;
  }

/*----- Searchbox -----*/
#portal-searchbox {
  background-color:#093650;
  border:1px solid #436174;
  font-size:75%;
  line-height:1.5em;
  margin:7px 15px 0 0;
  }
#portal-searchbox .LSBox {
  border-top:1px solid #072c41;
  padding:0 2px;
  position:relative;
  }
#portal-searchbox .searchField {
  background-color:#093650;
  border:0;
  color:#fff;
  font-size:1em;
  outline:none;
  padding:1px 0 0 4px;
  vertical-align:middle;
  width:115px;
  }
input.searchField {
  -moz-appearance:none;
  }
#portal-searchbox .searchButton {
  background:url(&dtml-portal_url;/sb.gif) no-repeat;
  border:0;
  height:0;
  overflow:hidden;
  padding-top:27px;
  vertical-align:middle;
  width:27px;
  }
.searchSection {
  display:none;
  }
#LSResult {
  background:none;
  margin:0;
  right:-2px;
  top:30px;
  }
#LSResult fieldset {
  border:0;
  margin:0;
  padding:0 0 1em;
  }
#livesearchLegend {
  background-color:#fff;
  border:1px solid #0d3b56;
  border-bottom:0;
  margin-left:-1px;
  left:0;
  margin:0;
  padding:2px 5px;
  position:absolute;
  top:-22px;
  }
.LSIEFix {
  background:#fff;
  border:1px solid #0d3b56;
  padding:7px 0;
  }
.LSRow,
#LSNothingFound {
  padding:0 1em;
  }

/*----- Global Navigation -----*/
#portal-globalnav {
  background:#012a43;
  clear:none;
  font-size:15px;
  line-height:23px;
  margin:0;
  }
#portal-globalnav li a {
  background-color:transparent;
  border:0;
  color:#fff;
  min-width:0;
  padding:11px 20px;
  }
#portal-globalnav li a:hover,
#portal-globalnav li.selected a,
#portal-globalnav li.selected:hover a {
  background-color:#6ea21b;
  color:#fff;
  }

/*----- VNC Collab Header -----*/
#vnccollab-header {
  background:url(&dtml-portal_url;/shadow.png) no-repeat 0 100%;
  border-bottom:1px solid #c1ccd5;
  margin: auto;
  padding:0 0 30px;
  position:relative;
  width: 960px;
  }
.nccollab-hp-header .actionLink {
  display:none;
  }
.hp-header-content {
  background:url(&dtml-portal_url;/vnccollab-header.jpg) no-repeat 0 0 #022c45;
  color:#fff;
  font-size:13px;
  height:323px;
  line-height:18px;
  padding:0 15px 0 325px;
  position:relative;
  }
.hp-header-welcome {
  padding:22px 0 0;
  }
.hp-header-welcome h2 {
  color:#fff;
  font-size:28px;
  font-weight:normal;
  line-height:35px;
  margin:0 0 14px;
  }
.hp-header-welcome p {
  margin:0 0 14px;
  }
.hp-header-first-link {
  background-color:#6b9512;
  bottom:0;
  height:89px;
  left:317px;
  padding:0 19px;
  position:absolute;
  width:286px;
  }
.hp-header-second-link {
  background-color:#164e6f;
  bottom:0;
  height:89px;
  left:641px;
  padding:0 19px;
  position:absolute;
  width:281px;
  }
.hp-header-first-link h3,
.hp-header-second-link h3 {
  font-size:28px;
  font-weight:normal;
  line-height:35px;
  margin:7px 0 14px;
  }
.hp-header-first-link h3 a,
.hp-header-second-link h3 a {
  color:#fff;
  }
#vnccollab-header.opened .headerTimeViewlet,
#vnccollab-header.opened .headerLinksViewlet,
#vnccollab-header.opened .carousel,
#vnccollab-header.opened .worldClockViewlet,
#vnccollab-header.closed .headerTimeViewletShort {
  display: block;
  }
#vnccollab-header.closed .headerTimeViewlet,
#vnccollab-header.closed .headerLinksViewlet,
#vnccollab-header.closed .carousel,
#vnccollab-header.closed .closeLink,
#vnccollab-header.closed .worldClockViewlet,
#vnccollab-header.opened .headerTimeViewletShort {
  display: none;
  }
.headerTimeViewlet {
  background:url(&dtml-portal_url;/clock.png) no-repeat 100% 0;
  color:#fff;
  float:right;
  font-size:1.06em;
  line-height:1.41em;
  margin:40px 0 0;
  padding:20px 80px 20px 20px;
  width:200px;
  }
.headerTimeViewlet span {
  display:inline-block;
  vertical-align:middle;
  }
.headerTimeViewlet .day {
  background-color:#fff;
  border-radius:15px;
  -moz-border-radius:15px;
  color:#082736;
  font-weight:bold;
  padding:4px 7px 3px 6px;
  }
.headerTimeViewletShort {
  background-color:#12425f;
  color:#fff;
  font-size:1.06em;
  line-height:1.41em;
  padding:2px 40px;
  text-align:right;
  }
.actionLink {
  background:url(&dtml-portal_url;/sprite.png) no-repeat;
  height:18px;
  overflow:hidden;
  position:absolute;
  right:11px;
  text-indent:-9999px;
  top:5px;
  width:18px;
  }
.openLink {
  background-position:0 -18px;
  text-indent:9999px;
  }
div.carousel {
  background-color:#12425f;
  margin:0;
  }
.carousel-banners {
  min-height: 117px;
  }
.carousel-image {
  float:left;
  }
div.carousel-text {
  background-color:transparent;
  color:#fff;
  display:table;
  font-size:0.81em;
  line-height:1.38em;
  padding:18px 40px 15px 22px;
  position:static;
  width:auto;
  }
div.carousel-text a:link,
div.carousel-text a:visited {
  color:#fff;
  text-decoration: underline;
  }
.carousel-title {
  color:#fff;
  display:block;
  font-size:1.46em;
  line-height:1.32em;
  margin:0 0 13px;
  }
.carousel-title:link,
.carousel-title:visited {
  color:#fff;
  }
div.carousel-pager {
  background-color:#a4b4c1;
  margin:0;
  padding:5px 15px;
  text-align:right;
  }
a.carousel-pager-button-prev,
a.carousel-pager-button-next {
  background-color:transparent;
  display:inline-block;
  padding:0;
  vertical-align:top;
  }
a.carousel-pager-button-prev {
  margin-right:5px;
  }
a.carousel-pager-button-prev span,
a.carousel-pager-button-next span {
  background:url(&dtml-portal_url;/sprite.png) no-repeat -18px 0;
  display:block;
  height:20px;
  overflow:hidden;
  text-indent:9999px;
  width:20px;
  }
a.carousel-pager-button-next span {
  background-position:-18px -20px;
  }
  
.headerLinksViewlet {
  bottom:10px;
  font-size:0.69em;
  line-height:1.45em;
  position:absolute;
  right:360px;
  }
.headerLinksViewlet li {
  margin-right:2px;
  }
.headerLinksViewlet a {
  background-color:#2a6181;
  border-radius:10px;
  -moz-border-radius:10px;
  color:#fff;
  display:inline-block;
  padding:2px 9px;
  }
.headerLinksViewlet a:visited {
  color:#fff;
  }

/*----- Bread Crumbs -----*/
#portal-breadcrumbs {
  background:url(&dtml-portal_url;/shadow.png) no-repeat 0 -2px;
  font-size:14px;
  line-height:20px;
  margin:0 0 15px;
  padding:15px 0 0;
  }
#breadcrumbs-you-are-here {
  background-color:#012a43;
  -moz-border-radius:9px 0 0 9px;
  border-radius:9px 0 0 9px;
  color:#fff;
  display:inline-block;
  margin-right:10px;
  padding:0 0 0 14px;
  position:relative;
  }
#breadcrumbs-you-are-here:after {
  background:url(&dtml-portal_url;/bc.gif) no-repeat 100% 0;
  content:"";
  display:block;
  height:20px;
  position:absolute;
  right:-10px;
  top:0;
  width:10px;
  }
#portal-breadcrumbs a,
#breadcrumbs-current {
  display:inline-block;
  color:#082736;
  padding:0 3px;
  }

/* XMPP Socializing Viewlet */
#xmpp-viewlet {
  padding-top:15px;
  }
#xmpp-viewlet a#toggle-online-users {
  border-radius:0 1em 1em 0;
  }
#xmpp-viewlet a#site-stream-link {
  border-radius:1em 0 0 1em;
  background-color:#012a43;
  color:#fff;
  text-shadow:1px 1px 1px #000;
  }
#xmpp-viewlet a#site-stream-link:hover {
  background-color:rgba(1, 42, 67, 0.8);
  }
#xmpp-viewlet-container > a#site-stream-link.newStreamMessage {
  background-color: darkOrange;
  color: white;
}
div#xmpp-viewlet-container > a {
  background-color:rgba(1, 42, 67, 0.2);
  display:inline-block;
  padding:0.35em 0.75em;
  margin-left:1px;
  }
div#xmpp-viewlet-container > a:hover {
  background-color:rgba(1, 42, 67, 0.1);
  }
#xmpp-viewlet-container a#toggle-online-users.active {
  background:rgba(1, 42, 67, 0.1);
  color:rgba(1, 42, 67, 1.0);
  -webkit-border-bottom-right-radius:0;
  border-bottom-right-radius:0;
  }
ul#online-users {
  background-color:#012a43;
  border:1px solid #012a43;
  color:#fff;
  box-shadow:0 1px 4px #000;
  }
ul#online-users li > a {
  color: #fff;
}
ul#online-users li > a:hover {
  color: #eee;
}

div.postItemWrapper {
  background-color:rgba(1, 42, 67, 0.3);
  border-bottom:1px solid #fff;
  -moz-border-radius:1em;
  border-radius:1em;
  margin:20px 0 1em 95px;
  max-width:574px;
  padding:1px;
  }
#content .postItemWrapper fieldset {
  border:0;
  -moz-border-radius:1em 1em 1em 0;
  border-radius:1em 1em 1em 0;
  padding:10px;
  }
#content .postItemWrapper textarea {
  font:18px "myriad pro", verdana, sans-serif;
  padding:5px;
  width:99%;
  }
.share-geolocation {
  color:#fff;
  margin-top:3px;
  text-shadow:0 1px 1px rgba(1, 42, 67, 0.8);
  }
.postItemWrapper form.pubsub-form button[type="submit"] {
  background:#012a43;
  border:0;
  -moz-border-bottomright-radius:11px;
  border-bottom-right-radius:11px;
  font-size:14px;
  margin-right:0;
  padding:0.6em 2em;
  }

/*------ Document Actions ------*/
#content-core,
.relatedItems,
.sc_social_bookmarks_viewlet,
.documentActions {
  margin-bottom: 10px;
  padding-bottom: 10px;
  background: url(&dtml-portal_url;/p-s.png) repeat-x 0 100%;
}

.sc_social_bookmarks_viewlet {
  display: none;
}

.sc_social_bookmarks_viewlet.visible {
  display: block;
}

.sc_social_bookmarks_wrapper {
  -moz-border-radius: 0.5em;
  -webkit-border-radius: 0.5em;
  border-radius: 0.5em;
  background-color: #ffffff;
}

/*------ Footer -----*/
#portal-footer {
  background:url(&dtml-portal_url;/f.png) no-repeat 0 0;
  display:table;
  padding:103px 0 0;
  width: 100%;
  }
.footerColumn {
  background:url(&dtml-portal_url;/f-s.png) no-repeat 0 5px;
  float:left;
  min-height:112px;
  padding:13px 30px 0 45px;
  width:250px;
  }
.footerColumn:first-child {
  background:none;
  padding-left:15px;
  }
.footerColumn dt {
  font-size:18px;
  font-weight:normal;
  line-height:18px;
  margin:0 0 18px;
  text-shadow:0 1px 1px #fff;
  }
#portal-footer ul {
  list-style-type:disc;
  margin:0 0 0 20px;
  }
#portal-footer li {
  display:list-item;
  }

/*----- Content -----*/
#content {
  position: relative;
  font-size:0.75em;
  line-height:1.5em;
  margin:0;
  }
#content .documentDescription,
#content #description {
  background:url(&dtml-portal_url;/p-s.png) repeat-x 0 100%;
  color:#082736;
  font-weight:normal;
  margin-bottom:25px;
  padding-bottom:20px;
  }
a:link,
#content a:link,
dl.portlet a:link {
  border-bottom:0;
  color:rgba(1, 42, 67, 0.6);
  }
a:visited,
#content a:visited {
  color:rgba(1, 42, 67, 0.6);
  }
#content a:hover,
dl.portlet a:hover,
#portal-footer a:hover {
  color:#012a43 !important;
  }
h1 {
  color:#012a43;
  font:35px/34px "Myriad Pro", Verdana, sans-serif;
  margin:0 0 10px;
  text-shadow:0px 1px 1px white;
  }
#content h1.documentFirstHeading {
  margin-top:9px;
  }
h1, h2 {
  letter-spacing:0;
  }

/*----- Portlets Properties -----*/
.portletWrapper {
  /*border-bottom:1px solid #dbe0e7;*/
  }
dl.portlet {
  background:url(&dtml-portal_url;/p-s.png) repeat-x 0 100%;
  /*border-bottom:1px solid #b6c2ce;*/
