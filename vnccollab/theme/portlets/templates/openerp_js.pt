<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      tal:omit-tag="">
<dl class="portlet portletOpenERPJS"
    i18n:domain="vnccollab.theme"
    tal:define="purl context/portal_url">

    <dt class="portletHeader">
        <span class="portletTopLeft"></span>
        <a href=""
           target="_blank"
           tal:attributes="href view/data/url"
           tal:content="view/title"
           class="tile">
            Tags
        </a>
        <span class="portletTopRight"></span>
    </dt>

    <dd class="portletBody openerp">
      <div class="openerp-content"
           tal:attributes="embedded_url  python:view.embed['embedded_url'];
                           url           view/embed/url;
                           login         view/embed/login;
                           dbname        view/embed/dbname;
                           key           view/embed/key;
                           action_id     view/embed/action_id;">
           Please wait a moment...</div>
    </dd>
</dl>
</html>
