<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      tal:omit-tag="">
<dl class="portlet portletOpenERPJS"
    i18n:domain="vnccollab.theme"
    tal:define="purl context/portal_url">

    <dt class="portletHeader">
        <span class="portletTopLeft"></span>
        <a href=""
           target="_blank"
           tal:attributes="href view/data/url"
           tal:content="view/title"
           class="tile">
            Tags
        </a>
        <span class="portletTopRight"></span>
    </dt>

    <dd class="portletBody openerp noSlimScroll">
      <iframe class="openerp-content"
           src="/VNC/openerp-iframe-helper"
           tal:attributes="src string:${context/@@plone_portal_state/navigation_root_url}/openerp-iframe-helper;
                           embedded_url  view/embedded_url;
                           url           view/url;
                           login         view/login;
                           dbname        view/dbname;
                           key           view/key;
                           action_id     view/action_id;">
      </iframe>
    </dd>
</dl>
</html>
